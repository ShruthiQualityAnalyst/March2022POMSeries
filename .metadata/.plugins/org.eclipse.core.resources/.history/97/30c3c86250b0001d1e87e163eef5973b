import com.abc.cart.pages.LoginPage;

public class BaseTest {
	
	public DriverFactory df;
	WebDriver driver;
	public LoginPage loginpage;
	
	@BeforeTest
	public void setup(){	
		
		//df = new Driverfactory();
		
		df = new DriverFactory();
		driver = df.init_driver("chrome");
	    loginpage = new LoginPage(driver);
		
	}

}
