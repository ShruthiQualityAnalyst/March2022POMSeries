package com.abc.cart.pages;

import org.openqa.selenium.By;

public class LoginPage {

	/*concept of encapsulation
	 * always use private for locators so that no other class can access it 
	 * it should be always accessible publicly
	 * 
	 */
	
	private By emailId = By.id("input-email");
	private By password = By.id("input-password");
	private By Login = By.xpath("//button[@class=\"btn btn-primary btn-lg btn-block visible-xs-block\"]");
	
	
}
